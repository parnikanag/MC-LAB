 AREA PGM1,CODE,READONLY
	ENTRY
	LDR R1,VALUE1
	LDR R2,VALUE2
	LDR R3,VALUE3
	LDR R4,VALUE4
	LDR R5,VALUE5
	MOV R6,R1
	
	ADD R7,R1,R2
	ADD R8,R3,#1
	
	ADDS R9,R4,R5
	ADC R10,R1,R2
	
	SUB R11,R2,#1
	MUL R12,R1,R2
	AND R5,R1,R2
	ORR R6,R1,R2
	EOR R7,R1,R2
	ORR R6,R1,R2
	EOR R7,R1,R2
	
STOP B STOP

VALUE1 DCD 0X00000001
VALUE2 DCD 0X00000002
VALUE3 DCD 0X00000003
VALUE4 DCD 0X00000004
VALUE5 DCD 0XFFFFFFFF
	END
