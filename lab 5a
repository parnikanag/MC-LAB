  AREA LARGEST, CODE, READONLY
	ENTRY
	MOV R5,#6
	LDR R1, =MYARRAY
	LDR R4, =RESULT
	LDR R2,[R1],#4
LOOP
	LDR R3,[R1],#4
	CMP R2,R3
	MOVLO R2,R3
	SUBS R5,R5,#1
	BNE LOOP
	STR R2,[R4]
STOP B STOP
MYARRAY
	DCD 0x44444444
	DCD 0x22222222
	DCD 0x11111111
	DCD 0x33333333
	DCD 0xAAAAAAAA
	DCD 0x88888888
	DCD 0x99999999
	AREA DATA2,DATA,READWRITE
RESULT DCD 0
	END 
